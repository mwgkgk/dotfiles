! URxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
! URxvt*.shading: 90

! Fixing flickering:
! URxvt*.transparent:  false
! URxvt*.inheritPixmap: false

URxvt.perl-ext-common: default,tabbedex
URxvt.tabbed.autohide: yes
URxvt.tabbed.title: false

URxvt*.scrollBar: false

! Alt keys:
URxvt*.meta8: true

! URxvt.foreground: DarkGoldenrod1
! URxvt*.foreground: #EBC593
! URxvt*.foreground: #ffaf00
! URxvt*.background: #1b1b1b
! URxvt*.foreground: #75F7EF
! URxvt*.foreground: #E6C29E
! URxvt*.foreground: #FFAF5F
! URxvt*.foreground: #DADADA
URxvt*.foreground: #DFA04A
URxvt*.background: #1b1b1b
! URxvt.depth: 24
! URxvt.background: [15]#000000
! URxvt*.cursorColor: #F76345
! URxvt*.cursorColor: #A75BC1
! URxvt*.cursorColor: #A08E6B
URxvt*.cursorColor: #995834

! URxvt*.tabbed.tab-bg: #98D53D
! URxvt*.tabbed.tab-bg: 5
URxvt*.color0: #3F3F3F
URxvt*.color9: #98D53D
URxvt*.color12: #8775E8
URxvt*.color4:  #3465A4
URxvt*.tabbed.tab-bg: 9

URxvt.keysym.Meta-F1: perl:tabbedex:prev_tab
URxvt.keysym.Meta-F2: perl:tabbedex:next_tab
URxvt.keysym.Meta-F3: perl:tabbedex:new_tab
URxvt.keysym.Meta-F4: perl:tabbedex:rename_tab

