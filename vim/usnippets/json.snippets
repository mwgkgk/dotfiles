snippet s "String" b
"${1:key}": "${0:value}",
endsnippet

snippet n "number" b
"${1:key}": ${0:value},
endsnippet

snippet a "Array" b
[
   ${VISUAL}$0
],
endsnippet
snippet o "Object" b
{
   ${VISUAL}$0
},
endsnippet

snippet v "video link" b
{
	"dateAdded" : "`date | tr -d "\n"`",
	"url" : "`!v @+`",
	"title" : `!v GetTitleFromUrl()`,
	"tags" : [ "music"$0 ]
},
endsnippet

snippet nodemon "nodemon config" b
{
    "events": {
        "restart": "${1:make}"
    }
}
endsnippet
