cd ~/bank/music/

colorscheme green
set lib_sort=filename

bind -f common q :q
bind -f common J :vol -5%
bind -f common K :vol +5%
bind -f common * :win-toggle
bind -f common space  :player-pause

set output_plugin=alsa
set dsp.alsa.device=default
set mixer.alsa.device=default
set softvol=true

set start_view=playlist
